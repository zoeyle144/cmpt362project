<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cmpt362project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CMPT362Project">
        <activity
            android:name=".ui.settings.UserProfileActivity"
            android:exported="false">
            <intent-filter>
                <action android:name="com.example.cmpt362project.ui.settings.UserProfileActivity"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:theme="@style/Theme.CMPT362Project.NoActionBar" />
        <activity
            android:name=".login.LoginPageActivity"
            android:exported="false" />
        <activity
            android:name=".login.SignUpPageActivity"
            android:exported="false" />
        <activity
            android:name=".StartPageActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.CMPT362Project.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>